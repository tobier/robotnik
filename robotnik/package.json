{
  "name": "robotnik",
  "version": "0.1.0",
  "description": "Friendly neighborhood Discord bot",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc --traceResolution",
    "start": "tsc && node dist/app.js",
